{
  "name": "@pearl.com/pearl-api-client-sdk",
  "version": "1.0.0",
  "description": "SDK for the Pearl API",
  "author": "Pearl.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Pearl-com/pearl-sdk.git",
    "directory": "sdk-typescript"
  },
  "bugs": {
    "url": "https://github.com/Pearl-com/pearl-sdk/issues"
  },
  "homepage": "https://github.com/Pearl-com/pearl-sdk/tree/main/sdk-typescript#readme",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rimraf dist && tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepare": "npm run build",
    "dev": "tsc --watch"
  },
  "keywords": [
    "pearl",
    "api",
    "sdk",
    "typescript",
    "node"
  ],
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/uuid": "^10.0.0",
    "jest": "^29.7.0",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "axios": "^1.9.0",
    "uuid": "^11.1.0"
  }
}
